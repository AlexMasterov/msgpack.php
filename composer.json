{
  "name": "alexmasterov/msgpack",
  "description": "A pure and efficient MessagePack serialization library",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/AlexMasterov/msgpack.php",
  "authors": [{
    "name": "Alex Masterov",
    "email": "alex.masterow@gmail.com"
  }],
  "keywords": ["msgpack", "messagepack", "pure"],
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "MessagePack\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MessagePack\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "7.2 - 7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  }
}
